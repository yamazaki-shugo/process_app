<div class="side-bar-header">
  <div class="header-name">
    <%= link_to current_user.nickname, "#"%>
  </div>
  <div class="create-room">
    <%= link_to 'ルームを作成する', new_room_path %>
  </div>
</div>

<div class="rooms">
  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="room-name">
        <%= link_to room.name, asign_room_path(room) %>
      </div>
    </div>
  <% end %>
</div>